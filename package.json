{
  "name": "@frederic-latour/fp-ts-mailerlite",
  "homepage": "https://github.com/FredericLatour/fp-ts-mailerlite",
  "version": "0.0.2",
  "description": "fp-ts based wrapper around Mailerlite API",
  "repository": {
    "type": "git",
    "url": "https://github.com/FredericLatour/fp-ts-mailerlite.git"
  },
  "author": "FredericLatour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FredericLatour/fp-ts-mailerlite/issues"
  },
  "keywords": [
    "typescript",
    "fp-ts",
    "mailerlite"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": "./dist/cjs/index.js",
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },  
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf ./dist/ && mkdir -p ./dist/esm/ ./dist/cjs/ && tsc -p tsconfig.build-esm.json && tsc -p tsconfig.build-cjs.json && tsc -p tsconfig.build-types.json",
    "build:watch": "pnpm tsc -w -p tsconfig.build.json",
    "prepublish": "pnpm build",
    "pretty": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "docu": "docs-ts",
    "spell": "cspell '**/*.*'",
    "md": "markdown"
  },
  "peerDependencies": {
    "fp-ts": "^2.9.5"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cspell": "^6.19.2",
    "docs-ts": "^0.7.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "fast-check": "^3.6.2",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "jest": "^29.4.1",
    "markdown-magic": "^2.6.1",
    "prettier": "^2.8.3",
    "prettier-plugin-jsdoc": "^0.4.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslog": "^4.7.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.5",
    "qs": "^6.11.0"
  }
}

